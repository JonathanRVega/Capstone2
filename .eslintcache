sets/icons/location.png'\n\nexport default class CreateMail extends Component {\n    constructor(props) {  \n        super(props); \n        \n        this.onChangeFirstName = this.onChangeFirstName.bind(this);\n        this.onChangeLastName = this.onChangeLastName.bind(this);\n        this.onChangeEmailAddress = this.onChangeEmailAddress.bind(this);\n        this.onChangeDescription = this.onChangeDescription.bind(this);\n        this.onSubmit = this.onSubmit.bind(this);    \n\n        this.state = {  \n          firstname: '',  \n          lastname: '',  \n          emailaddress: '',  \n          description: '',  \n          mail: []\n        }  \n      }\n\n    componentDidMount() {\n        this.setState({\n            mail: ['test mail'],\n        })\n    }\n\n    onChangeFirstName(e) {\n        this.setState({\n            firstname: e.target.value\n        });\n    }\n    \n    onChangeLastName(e) {\n        this.setState({\n            lastname: e.target.value\n        });\n    }\n\n    onChangeEmailAddress(e) {\n        this.setState({\n            emailaddress: e.target.value\n        });\n    }\n\n    onChangeDescription(e) {\n        this.setState({\n            description: e.target.value\n        });\n    }\n\n    onSubmit(e) {\n        e.preventDefault();\n\n        const mail = {\n            firstname: this.state.firstname,\n            lastname: this.state.lastname,\n            emailaddress: this.state.emailaddress,\n            description: this.state.description\n        }\n\n        console.log(mail);\n\n        axios.post('http://localhost:5000/mail/add', mail)\n            .then(res => console.log(res.data));\n            \n\n        // window.location = '/';\n    }\n\n    render() {\n        return (\n            <div className=\"contact-header-container container\">\n                <h2 className=\"text-center\">Questions?</h2>\n                <h1 style={{color:'#FE5000'}} className=\"text-center mb-5\">Contact Us</h1>\n                <p className=\"text-center mb-5 mt-5\">Aliqua occaecat ea laborum culpa deserunt Lorem sint.</p>\n                <div className=\"row mb-5\">\n                    <div className=\"col\">\n                        <img className=\"mr-2\" style={{width:'3rem'}}src={phone}/>\n                        <h4 style={{display: 'inline-block'}}>1-800-555-3692</h4>\n                    </div>\n                    <div className=\"col\">\n                        <img className=\"mr-2\" style={{width:'3rem'}}src={email}/>\n                        <h4 style={{display: 'inline-block'}}>Explorecincy@explore.com</h4>\n                    </div>\n                    <div className=\"col\">\n                        <img className=\"mr-2\" style={{width:'3rem'}}src={location}/>\n                        <h4 style={{display: 'inline-block'}}>Cincinnati, Ohio</h4>\n                    </div>\n                </div>\n                <form className=\"formToRight\" onSubmit={this.onSubmit}>\n                    <div className=\"form-group\"> \n                        <div className=\"row\">\n                            <div className=\"col\">\n                                <input  type=\"text\"\n                                required\n                                className=\"form-control\"\n                                placeholder=\"First Name\"\n                                value={this.state.firstname}\n                                onChange={this.onChangeFirstName}\n                                />\n                            </div>\n                            <div className=\"col\">\n                                <input  type=\"text\"\n                                required\n                                className=\"form-control\"\n                                placeholder=\"Last Name\"\n                                value={this.state.lastname}\n                                onChange={this.onChangeLastName}\n                                />\n                            </div>\n                        </div>\n                    </div>\n                    <div className=\"form-group\">\n                        <input \n                            type=\"text\" \n                            className=\"form-control\"\n                            placeholder=\"Email Address\"\n                            value={this.state.emailaddress}\n                            onChange={this.onChangeEmailAddress}\n                            />\n                    </div>\n                    <div className=\"form-group\">\n                        <textarea \n                            type=\"text\" \n                            className=\"form-control form-description\"\n                            placeholder=\"Description\"\n                            value={this.state.description}\n                            onChange={this.onChangeDescription}\n                        />\n                    </div>\n            \n                    <div className=\"form-group\">\n                        <input type=\"submit\" value=\"Submit\" className=\"btn contactSubmit\" />\n                    </div>\n                </form>\n            </div>\n        )\n    }\n}\n","/Users/macbookair/Documents/KA Projects/CapstoneCincinnati/src/components/attractions-list.component.js",[],"/Users/macbookair/Documents/KA Projects/CapstoneCincinnati/src/components/create-attraction.component.js",[],"/Users/macbookair/Documents/KA Projects/CapstoneCincinnati/src/components/StayCarousel.js",[],"/Users/macbookair/Documents/KA Projects/CapstoneCincinnati/src/components/FrontEndComponents/ToDo.js",[],"/Users/macbookair/Documents/KA Projects/CapstoneCincinnati/src/components/FrontEndComponents/Eat.js",[],"/Users/macbookair/Documents/KA Projects/CapstoneCincinnati/src/components/FrontEndComponents/Play.js",[],"/Users/macbookair/Documents/KA Projects/CapstoneCincinnati/src/components/FrontEndComponents/Shop.js",[],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80